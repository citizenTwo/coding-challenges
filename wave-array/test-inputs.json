{
    "testCases": [
        {
            "input": [1, 2, 3, 4],
            "expected": [2, 1, 4, 3],
            "description": "Basic even-length array"
        },
        {
            "input": [1, 5, 1, 1, 6, 4],
            "expected": [5, 1, 6, 1, 4, 1],
            "description": "Array with duplicates"
        },
        {
            "input": [2, 2, 2],
            "expected": [2, 2, 2],
            "description": "Array with all equal elements"
        },
        {
            "input": [],
            "expected": [],
            "description": "Empty array"
        },
        {
            "input": [5],
            "expected": [5],
            "description": "Single element array"
        },
        {
            "input": [1, 2],
            "expected": [2, 1],
            "description": "Two element array"
        },
        {
            "input": [4, 3, 2, 1, 5],
            "expected": [4, 2, 5, 1, 3],
            "description": "Odd-length array"
        },
        {
            "input": [-1, -2, -3, -4],
            "expected": [-2, -4, -1, -3],
            "description": "Array with negative numbers"
        },
        {
            "input": [10, 5, 6, 3, 2, 20, 100, 80],
            "expected": [10, 5, 6, 2, 20, 3, 100, 80],
            "description": "Array with large numbers"
        },
        {
            "input": [1, 1, 1, 2, 2, 2],
            "expected": [1, 1, 2, 1, 2, 2],
            "description": "Array with multiple duplicates"
        }
    ]
}